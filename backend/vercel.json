{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "api/generate-emoji": {
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}
